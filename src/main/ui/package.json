{
    "name": "science-research",
    "version": "4.2.1",
    "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
    "author": "SJTU NIC",
    "license": "MIT",
    "scripts": {
        "dev": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:ui": "vue-cli-service build && npm run copy",
        "copy": "cp -R dist/static/ui/ ../webapp/static/ui/ && cp dist/index.html ../webapp/WEB-INF/ui/index.html && cp dist/favicon.ico ../webapp/favicon.ico",
        "build:stage": "vue-cli-service build --mode staging && npm run copy",
        "preview": "node build/index.js --preview",
        "lint": "eslint --ext .js,.vue src",
        "lintfix": "vue-cli-service lint --fix",
        "test:unit": "jest --clearCache && vue-cli-service test:unit",
        "test:ci": "npm run lint && npm run test:unit",
        "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
    },
    "dependencies": {
        "axios": "0.18.1",
        "core-js": "^2.6.12",
        "js-cookie": "2.2.0",
        "normalize.css": "7.0.0",
        "nprogress": "0.2.0",
        "path-to-regexp": "2.4.0",
        "vue": "2.6.10",
        "vue-router": "3.0.6",
        "vuex": "3.1.0"
    },
    "devDependencies": {
        "@babel/core": "7.0.0",
        "@babel/register": "7.0.0",
        "@vue/cli-plugin-babel": "3.6.0",
        "@vue/cli-plugin-eslint": "^3.9.1",
        "@vue/cli-plugin-unit-jest": "3.6.3",
        "@vue/cli-service": "3.6.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "autoprefixer": "^9.5.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^8.2.2",
        "babel-jest": "23.6.0",
        "chalk": "2.4.2",
        "connect": "3.6.6",
        "eslint": "5.15.3",
        "eslint-plugin-vue": "5.2.2",
        "html-webpack-plugin": "3.2.0",
        "node-sass": "^6.0.1",
        "runjs": "^4.3.2",
        "sass-loader": "^10.2.0",
        "script-ext-html-webpack-plugin": "2.1.3",
        "script-loader": "^0.7.2",
        "serve-static": "^1.13.2",
        "svg-sprite-loader": "4.1.3",
        "svgo": "1.2.2",
        "vue-template-compiler": "2.6.10"
    },
    "engines": {
        "node": ">=8.9",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
